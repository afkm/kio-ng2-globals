		"@angular/core": "~4.2.3",
		"@angular/platform-browser": "~4.2.3",
		"@angular/platform-browser-dynamic": "~4.2.3",
		"@angular/router": "~4.2.3",
		"cuid": "^1.3.8",
		"kio-ng2-component-routing": "github:afkm/kio-ng2-component-routing#v0.1.2",
		"kio-ng2-ctn": "github:afkm/kio-ng2-ctn#v0.0.7",
		"kio-ng2-data": "github:afkm/kio-ng2-data#v0.0.1",
		"kio-ng2-i18n": "github:afkm/kio-ng2-i18n#v0.0.2",
		"rxjs": "^5.4.3",
		"zone.js": "^0.8.17"
	},
	"devDependencies": {
